def main_menu():
    while True:
        main_banner()
        
        table = Table(title="CH·ª®C NƒÇNG B√ôI ƒê√åNH B√åNH", title_style="bold bright_yellow", border_style="bright_cyan")
        table.add_column("M√£", style="bold green", justify="center")
        table.add_column("Ch·ª©c nƒÉng", style="bold white")
        table.add_column("M√¥ t·∫£", style="magenta")
        
        table.add_row("[1]", "FACEBOOK SHARE LI√äN T·ª§C", "TƒÉng l∆∞·ª£t Share cho b√†i vi·∫øt ")
        table.add_row("[2]", "CODELAG ‚öî", "Spam/G·ª≠i tin nh·∫Øn nhanh v√†o Box ")
        table.add_row("[3]", "SMS SPAM / OTP BOMBER üí£", "G·ª≠i h√†ng lo·∫°t OTP ƒë·∫øn SƒêT n·∫°n nh√¢n")
        table.add_row("---", "---", "---")
        table.add_row("[0]", "THO√ÅT TOOL üïä", "ƒê√≥ng ch∆∞∆°ng tr√¨nh", style="bold red")

        console.print(table, justify="center")
        
        try:
            chon = console.input(f"{Colors.GREEN}‚û© {Colors.WHITE}Ch·ªçn m√£ ch·ª©c nƒÉng: {Colors.YELLOW}")
            if not chon.isdigit(): raise ValueError
            chon = int(chon)
            
            if chon == 1:
                start_share_tool()
            elif chon == 2:
                run_codelag_tool()
            elif chon == 3:
                start_spam_sms_tool()
            elif chon == 0:
                print(f"{Colors.GREEN}[B√πi ƒê√¨nh Tool] Tho√°t tool th√†nh c√¥ng. H·∫πn g·∫∑p l·∫°i!")
                sys.exit()
            else:
                print(f"{Colors.RED}[L·ªñI] L·ª±a ch·ªçn kh√¥ng h·ª£p l·ªá! Vui l√≤ng ch·ªçn l·∫°i.")
                sleep(2)
                
        except ValueError:
            print(f"{Colors.RED}[L·ªñI] Vui l√≤ng nh·∫≠p s·ªë!")
            sleep(2)
        except KeyboardInterrupt:
            print(f"\n{Colors.GREEN}[B√πi ƒê√¨nh Tool] Tho√°t tool th√†nh c√¥ng.")
            sys.exit()


if __name__ == "__main__":
    # ƒê·∫£m b·∫£o c√°c th∆∞ vi·ªán c·∫ßn thi·∫øt ƒë√£ ƒë∆∞·ª£c c√†i ƒë·∫∑t
    try:
        import rich
        import colorama
    except ImportError:
        print("L·ªói: C√°c th∆∞ vi·ªán 'rich' ho·∫∑c 'colorama' ch∆∞a ƒë∆∞·ª£c c√†i ƒë·∫∑t. Vui l√≤ng ch·∫°y: pip install rich colorama")
        sys.exit(1)
        
    main_menu()
