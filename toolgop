        f"{Colors.YELLOW} ‚ûú {Colors.WHITE}Admin‚≠ê: B√¨nh üíå(\n"
        f"{Colors.YELLOW} ‚ûú {Colors.WHITE}Zalo : üî∞0377994308.\n"
        f"{Colors.YELLOW} ‚ûú {Colors.WHITE}TOOL G·ªòP üå†üí¨\n"
        f"{Colors.YELLOW} ‚ûú {Colors.WHITE}CH·ª®C NƒÇNG CODELAG‚öî\n"
        f"{Colors.CYAN}‚îî‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îò\n"
    )
    print(banner_codelag)

    group_id = console.input(f"{Colors.GREEN}-> ID Box: {Colors.WHITE}")
    cookie = console.input(f"{Colors.GREEN}-> Cookie Facebook: {Colors.WHITE}")
    
    try:
        delay = float(console.input(f"{Colors.GREEN}-> Delay (gi√¢y): {Colors.WHITE}"))
    except:
        delay = 5
        print(f"{Colors.YELLOW}Kh√¥ng nh·∫≠p ho·∫∑c nh·∫≠p sai, m·∫∑c ƒë·ªãnh Delay: 5 gi√¢y.{Colors.END}")

    try:
        # Th·ª≠ t√¨m codelag.txt ·ªü th∆∞ m·ª•c hi·ªán t·∫°i
        with open('codelag.txt', 'r', encoding='utf-8') as f:
            lines = f.readlines()
    except FileNotFoundError:
        print(f"{Colors.RED}Kh√¥ng t√¨m th·∫•y file 'codelag.txt'. Vui l√≤ng t·∫°o file n√†y v√† nh·∫≠p n·ªôi dung c·∫ßn spam.")
        sleep(3)
        return
    except Exception as e:
        print(f"{Colors.RED}L·ªói khi ƒë·ªçc file 'codelag.txt': {e}")
        sleep(3)
        return

    try:
        messenger = Messenger(cookie)
        for line in lines:
            message = line.strip()
            if message:
                success = messenger.send_message(group_id, message)
                print(f"{Colors.YELLOW}G·ª≠i codelag: {message} => {Colors.GREEN if success else Colors.RED}{'Th√†nh c√¥ng' if success else 'Th·∫•t b·∫°i'}{Colors.END}")
                sleep(delay)
    except Exception as e:
        print(f"{Colors.RED}[L·ªñI] X·∫£y ra l·ªói trong qu√° tr√¨nh g·ª≠i: {e}{Colors.END}")

    sleep(3)

