# ==============================================================================
# --- PH·∫¶N 1: FACEBOOK SHARE LI√äN T·ª§C (T·ª™ l.py) ---
# ==============================================================================

success_count = 0
lock = asyncio.Lock() 

async def getid(session, link):
    async with session.post('https://id.traodoisub.com/api.php', data={"link": link}) as response:
        rq = await response.json()
        if 'success' in rq: return rq["id"]
        else: console.print(f"[red]Link post sai!!! Vui l√≤ng nh·∫≠p l·∫°i"); return None

async def get_token(session, token, cookie):
    params = {'access_token': token}
    headers = {'cookie': cookie, 'user-agent': 'Mozilla/5.0'}
    async with session.get('https://graph.facebook.com/me/accounts', params=params, headers=headers) as r:
        rq = await r.json()
        if 'data' in rq: return rq
        else: console.print(f"[red]Token Ho·∫∑c Cookie Sai! L·ªói khi l·∫•y page cho token n√†y. Vui l√≤ng ki·ªÉm tra l·∫°i."); return {}

async def share_single_post(session, tk, ck, post, published_value):
    headers = {'cookie': ck, 'user-agent': 'Mozilla/5.0'}
    async with session.get(f'https://graph.facebook.com/me/feed?method=POST&link=https://m.facebook.com/{post}&published={published_value}&access_token={tk}', headers=headers) as response:
        json_data = await response.json()
        if 'id' in json_data: return True, json_data.get('id', 'N/A')
        else: return False, json_data.get('error', {}).get('message', 'Unknown error')

async def share_loop(session, tk, ck, post, page_id):
    global success_count
    current_published_status = 0
    consecutive_block_count = 0 
    
    while True:
        try:
            now = datetime.datetime.now()
            current_time = now.strftime("%H:%M:%S")
            is_success, result = await share_single_post(session, tk, ck, post, current_published_status)
            
            if is_success:
                async with lock:
                    success_count += 1
                    current_success_count = success_count
                consecutive_block_count = 0 
                console.print(f"[green]| Th√†nh c√¥ng | [magenta]{current_time} | [blue]{page_id} | [cyan]published={current_published_status} | [yellow]{current_success_count} |")
                
            else:
                consecutive_block_count += 1
                error_message = result 
                
                if consecutive_block_count == 1:
                    next_published_status = 1 if current_published_status == 0 else 0
                    console.print(f"[yellow]| Th·∫•t b·∫°i | [magenta]{current_time} | [blue]{page_id} | [red]published={current_published_status} | [yellow]Chuy·ªÉn sang published={next_published_status} v√† th·ª≠ l·∫°i... |")
                    current_published_status = next_published_status
                    await asyncio.sleep(1)
                    continue
                
                elif consecutive_block_count == 2:
                    current_published_status = 0
                    console.print(f"[red]| Th·∫•t b·∫°i 2 l·∫ßn | [magenta]{current_time} | [blue]{page_id} | [red]C·∫£ published=0 v√† published=1 ƒë·ªÅu b·ªã ch·∫∑n! T·∫°m d·ª´ng 30 ph√∫t. |")
                    await asyncio.sleep(1800)
                    consecutive_block_count = 0
                    continue

                console.print(f"[red]| L·ªói Share | [magenta]{current_time} | [blue] {page_id} | L·ªói: {error_message} | Th·ª≠ l·∫°i sau 10 gi√¢y.")
                await asyncio.sleep(10)
                
        except Exception as e:
            console.print(f"| [red]L·ªói ngo·∫°i l·ªá | [magenta]{datetime.datetime.now().strftime('%H:%M:%S')} | [blue] {page_id} | [red]{e} | Th·ª≠ l·∫°i sau 60 gi√¢y...")
            await asyncio.sleep(60)

async def run_share_tool(link, tokens_input, cookie_input):
    global success_count
    success_count = 0 
    async with aiohttp.ClientSession() as session:
        post = await getid(session, link)
        if not post: return

        tokens = [t.strip() for t in tokens_input.split('|') if t.strip()]
        cookies = [c.strip() for c in cookie_input.split('|') if c.strip()]
        
        if not tokens or not cookies:
            console.print(f"[red]Vui l√≤ng nh·∫≠p Token v√† Cookie h·ª£p l·ªá.")
            return

        list_pages = []
        cookie_for_request = cookies[0] 
        
        for token in tokens:
            token_data = await get_token(session, token, cookie_for_request)
            
            if 'data' in token_data:
                for page in token_data['data']:
                    list_pages.append({"tk": page["access_token"], "page_id": page["id"], "ck": cookie_for_request})
        
        if not list_pages:
            console.print(f"[red]Kh√¥ng t√¨m th·∫•y Page n√†o t·ª´ c√°c token ƒë√£ cung c·∫•p.")
            return
            
        main_banner()
        console.print(f"[magenta]T·ªïng S·ªë Token Page: [yellow]{len(list_pages)}")
        
        tasks = [asyncio.create_task(share_loop(session, page["tk"], page["ck"], post, page["page_id"])) for page in list_pages]
        
        await asyncio.gather(*tasks)

def start_share_tool():
    main_banner()
    console.print(f"{Colors.YELLOW}--- CH·ª®C NƒÇNG: FACEBOOK SHARE LI√äN T·ª§C (ASYNCHRONOUS) ---{Colors.END}")

    link = console.input("[green]Link Post: [yellow]")
    tokens_input = console.input(f"{Colors.GREEN}Nh·∫≠p Token (D√πng d·∫•u '|' ƒë·ªÉ ngƒÉn c√°ch): [yellow]")
    cookie_input = console.input(f"{Colors.GREEN}Nh·∫≠p Cookie (Ch·ªâ c·∫ßn 1 cookie ƒë·ªÉ l·∫•y page): [yellow]")
    
    try:
        # Ki·ªÉm tra n·∫øu ƒëang ch·∫°y trong m√¥i tr∆∞·ªùng Jupyter/IPython
        try:
            get_ipython()
            asyncio.run(run_share_tool(link, tokens_input, cookie_input))
        except NameError:
            # N·∫øu kh√¥ng ph·∫£i Jupyter/IPython
            asyncio.run(run_share_tool(link, tokens_input, cookie_input))

    except KeyboardInterrupt:
        console.print("[yellow]Ch∆∞∆°ng tr√¨nh b·ªã d·ª´ng b·ªüi ng∆∞·ªùi d√πng (Ctrl+C). Quay l·∫°i Menu ch√≠nh.")
    except Exception as e:
        console.print(f"[red]ƒê√£ x·∫£y ra l·ªói kh√¥ng mong mu·ªën: {e}")
    sleep(3)

# ==============================================================================
# --- PH·∫¶N 2: CODELAG (T·ª™ l.py) ---
# ==============================================================================

class Messenger:
    def __init__(self, cookie):
        self.cookie = cookie
        self.user_id = self.get_user_id()
        self.fb_dtsg = None
        self.init_params()

    def get_user_id(self):
        try: return re.search(r"c_user=(\d+)", self.cookie).group(1)
        except: raise Exception("Cookie kh√¥ng h·ª£p l·ªá")

    def init_params(self):
        headers = {'Cookie': self.cookie, 'User-Agent': 'Mozilla/5.0'}
        try:
            response = requests.get('https://m.facebook.com', headers=headers)
            match = re.search(r'name="fb_dtsg" value="(.*?)"', response.text)
            if match: self.fb_dtsg = match.group(1)
            else: raise Exception("Kh√¥ng t√¨m th·∫•y fb_dtsg")
        except Exception as e: raise Exception(f"L·ªói kh·ªüi t·∫°o: {str(e)}")

    def send_message(self, recipient_id, message):
        timestamp = int(time.time() * 1000)
        data = {
            'fb_dtsg': self.fb_dtsg, '__user': self.user_id, 'body': message,
            'action_type': 'ma-type:user-generated-message', 'timestamp': timestamp,
            'offline_threading_id': str(timestamp), 'message_id': str(timestamp),
            'thread_fbid': recipient_id, 'source': 'source:chat:web', 'client': 'mercury'
        }
        headers = {
            'Cookie': self.cookie, 'User-Agent': 'Mozilla/5.0',
            'Content-Type': 'application/x-www-form-urlencoded'
        }
        try:
            r = requests.post('https://www.facebook.com/messaging/send/', data=data, headers=headers)
            return r.status_code == 200
        except: return False

